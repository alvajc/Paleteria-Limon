doctype html
html(ng-app='store')
  head
    title Paleteria Limon
    link(href='https://fonts.googleapis.com/css?family=Mr+Dafoe', rel='stylesheet', type='text/css')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', href='../css/default.css')
    script(src='https://use.fontawesome.com/62e7e9ea00.js')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
  body

    // NavBar

    nav.navbar.navbar-default.navbar-fixed-top(role='navigation')
      .container-fluid
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#')
           img.navImage(src='../images/Artboard 3.svg')

        // Collect the nav links, forms, and other content for toggling
        .collapse.navbar-collapse#bs-example-navbar-collapse-1
          ul.nav.navbar-nav
            li
              a#navtext(href='#menu').text-center  Menu
            li
              a#navtext(href='#contactUs').text-center  Contact Us
            li
              a#navtext(href='#followUs').text-center  Follow Us

